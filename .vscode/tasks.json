{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "web:serve",
      "type": "npm",
      "script": "start",
      "path": "WebClient",
      "isBackground": true,
      "problemMatcher": {
        "base": "$tsc-watch",
        "background": {
          "activeOnStart": true,
          "beginsPattern": "Starting development server|Angular Live Development Server|Vite dev server running|√|✔",
          "endsPattern": "Local:\\s*http://localhost:4200/?|Compiled successfully"
        }
      }
    }
  ]
}
